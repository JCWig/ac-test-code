{
  "extends": "akamai",

  // TODO: these really need to be enabled, but they cause about 80 errors and 200 warnings
  "rules": {
    "arrow-spacing": 0,
    "constructor-super": 0,
    "no-class-assign": 0,
    "no-const-assign": 0,
    "no-dupe-class-members": 0,
    "object-shorthand": 0,
    "no-this-before-super": 0,
    "prefer-arrow-callback": 0,
    "prefer-const": 0,
    "prefer-spread": 0,
    "prefer-reflect": 0,
    "prefer-template": 0,
    "require-yield": 0
  }
}